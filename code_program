
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = input ('введите депозит')
i=0
deposit = []
print ('за год вы получите депозит , Банк: размер депозита')
for key, value in per_cent.items() :
    deposit.append(float(value)*float(money))
    print(key, ': ', deposit[i])
    i = i+1
print('Максимальная сумма, которую вы можете заработать ')
print (max(deposit))
